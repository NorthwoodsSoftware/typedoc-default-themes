{{#unless settings.hideGenerator}}
    <footer class="with-border-bottom">
        <div class="container plr15">
            <h2>Legend</h2>
            <div class="tsd-legend-group">
                {{#each legend}}
                    <ul class="tsd-legend">
                        {{#each .}}
                            <li class="{{#compact}}{{#each classes}} {{.}}{{/each}}{{/compact}}"><span class="tsd-kind-icon">{{name}}</span></li>
                        {{/each}}
                    </ul>
                {{/each}}
            </div>
        </div>
    </footer>

    <div class="container tsd-generator plr15">
        <p>Generated using <a href="https://typedoc.org/" target="_blank">TypeDoc</a></p>
    </div>
{{/unless}}
<div class="container bottom-copyright plr15">
    Copyright &copy; 1998-2021 by Northwoods Software Corporation.
</div>